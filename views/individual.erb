<h1><%= title.capitalize  %></h1>
<% videos = videos.split("\r\n") %>
<% videos.each do |vid| %>
	<iframe id="vid" width="400" height="300" src= <%="https://www.youtube.com/embed/"+vid.to_s%> frameborder="0" allowfullscreen></iframe>
<% end %>
<form method="post" action="/sets"> 
	<input type= "text" name="title" placeholder = "Title Here" value = <%=title%>>
	<input  name="_method" value="delete" type="submit" action="/sets">
</form>
